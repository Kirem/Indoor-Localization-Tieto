[project]
	description = Projekt zespolowy poruszaj?cy zagadnienie indoor localization na kampusie Politechniki Wroc?awskiej
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Kirem
[access "refs/for/refs/*"]
	read = group user/Kirem
	push = group user/Kirem
	label-Code-Review = -2..+2 group user/Kirem
	label-Verified = -1..+1 group user/Kirem
	rebase = group user/Kirem
	removeReviewer = group user/Kirem
	submit = group user/Kirem
[access "refs/heads/*"]
	read = group user/Kirem
	create = group user/Kirem
	push = +force group user/Kirem
	pushMerge = group user/Kirem
[access "refs/tags/*"]
	push = +force group user/Kirem
	pushTag = group user/Kirem
	pushSignedTag = group user/Kirem
